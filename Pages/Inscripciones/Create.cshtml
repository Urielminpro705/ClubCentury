@page
@model ClubCentury.Pages.Inscripciones.CreateModel

@{
    ViewData["Title"] = "Crear-Inscripcion";
}

<h1>Crear</h1>

<h4>Inscripcion</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Inscripcion.TallerID" class="control-label"></label>
                <select asp-for="Inscripcion.TallerID" class ="form-control" asp-items="ViewBag.TallerID"></select>
                <span asp-validation-for="Inscripcion.TallerID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Inscripcion.SocioID" class="control-label"></label>
                <select asp-for="Inscripcion.SocioID" class ="form-control" asp-items="ViewBag.SocioID"></select>
                <span asp-validation-for="Inscripcion.SocioID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Volver a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
