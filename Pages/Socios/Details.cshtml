@page
@model ClubCentury.Pages.Socios.DetailsModel

@{
    ViewData["Title"] = "Detalles-Socio";
}

<h1>Detalles</h1>

<div>
    <h4>Socio</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.SocioID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.SocioID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.ClubID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.ClubID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.PaqueteID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.PaqueteID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Apellido_Paterno)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Apellido_Paterno)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Apellido_Materno)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Apellido_Materno)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Curp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Curp)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Domicilio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Domicilio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Telefono)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Telefono)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Correo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Correo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio.Inscripciones)
        </dt>
        <dd class="col-sm-10">
            <table class="table">
                <tr>
                    <th>Nombre del Taller</th>
                </tr>
                @foreach (var item in Model.Socio.Inscripciones)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Taller.Nombre)
                        </td>
                    </tr>
                }
            </table>
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Socio.SocioID">Editar</a> |
    <a asp-page="./Index">volver a la lista</a>
</div>
